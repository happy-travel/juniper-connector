{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "juniper-connector",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Database": {
    "ConnectionString": "Server={0};Port={1};Database=juniper;Userid={2};Password={3};",
    "Options": "juniper-connector/connection-string"
  },
  "AllowedHosts": "*"
}
